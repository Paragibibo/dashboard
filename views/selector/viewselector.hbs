<h3> Events List</h3>
<table  class="table table-dark">
    <thead>
        <tr>
            <th>Action</th>
            <th>Selector</th>
            <th>Href</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{this.action}}</td>
            <td>{{this.selector}}</td>
            <td>
                {{this.href}}</td>
            <td>
                <a onclick="return confirm('Are you sure to delete this {{this.action}} ?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
          
        </tr>
        {{/each}}
    </tbody>
</table>